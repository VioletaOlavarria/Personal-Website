:root{
	/* Active window */
	--activetitle: Cyan;
	--gradientactivetitle: Green;
	--titletext: White;
	--activeborder: rgb(192, 192, 192);
 
  cursor:url("https://i.imgur.com/R7oGLnR.png"),default;

	/* Scrollbar */
	--scrollbar: rgb(192, 192, 192); cursor:url("https://i.imgur.com/R7oGLnR.png"),default;
  
  

	/* Inactive window */
