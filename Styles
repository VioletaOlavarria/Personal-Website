	--gradientinactivetitle: rgb(128, 128, 120);
	--inactivetitletext: rgb(192, 192, 192);
	--inactiveborder: rgb(192, 192, 192);
  cursor:url("https://i.imgur.com/R7oGLnR.png"),default;

